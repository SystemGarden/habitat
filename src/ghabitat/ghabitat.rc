style "default-text"
{
	fontset = "-*-helvetica-medium-r-*-*-*-100-*-*-*-*-*-*"
}

style "text"
{
	bg[NORMAL] = "#ffffff"
	fg[NORMAL] = "#003366"
}

style "splash"
{
	font = "-*-helvetica-bold-r-normal-*-34-240-*-*-*-*-*-*"
	fontset = "-*-helvetica-bold-r-normal-*-34-240-*-*-*-*-*-*"
#	font = "-*-helvetica-bold-r-normal-*-*-480-*-*-*-*-*-*"
	fg[NORMAL] = "#006699"
}

style "about"
{
	font = "-*-helvetica-bold-r-normal-*-34-240-*-*-*-*-*-*"
	fontset = "-*-helvetica-bold-r-normal-*-34-240-*-*-*-*-*-*"
	bg[NORMAL] = "#ffffff"
	fg[NORMAL] = "#006699"
}

style "editable"
{
	fg[NORMAL] = "#ff0000"
}

style "dynamic"
{
	font = "-*-helvetica-medium-r-*-*-*-100-*-*-*-*-*-*"
	fontset = "-*-helvetica-medium-r-*-*-*-100-*-*-*-*-*-*"
	fg[NORMAL] = "#0000ff"
}

style "menutitle"
{
	font = "-*-helvetica-bold-r-*-*-*-120-*-*-*-*-*-*"
	fontset = "-*-helvetica-bold-r-*-*-*-120-*-*-*-*-*-*"
	fg[NORMAL] = "#000000"
	fg[PRELIGHT] = "#000000"
	fg[ACTIVE] = "#000000"
	fg[SELECTED] = "#000000"
}

style "graph"
{
	font = "-*-helvetica-medium-r-*-*-*-80-*-*-*-*-*-*"
	fontset = "-*-helvetica-medium-r-*-*-*-80-*-*-*-*-*-*"
	bg[NORMAL] = "#ffffff"
	bg[PRELIGHT] = "#ffffff"
	bg[ACTIVE] = "#ffffff"
	bg[SELECTED] = "#ffffff"
	fg[NORMAL] = "#003366"
	fg[PRELIGHT] = "#003366"
	fg[ACTIVE] = "#003366"
	fg[SELECTED] = "#003366"
}

style "pickstyle"
{
#	font = "-adobe-helvetica-bold-r-normal-*-34-240-*-*-p-*-iso8859-1"
	bg[SELECTED] = "#ccb5cc"
#	base[SELECTED] = "#ccb5cc"
}

widget "baseWindow.*splash_view" style "text"
widget "baseWindow.*splashprod" style "splash"
widget "baseWindow.*graphframe" style "graph"
widget "baseWindow.*gmcgraph*" style "graph"
widget "baseWindow.*ednode" style "editable"
widget "baseWindow.*dynode" style "dynamic"
widget "about_dialog" style "about"
widget "*choice_popup_label*" style "menutitle"


