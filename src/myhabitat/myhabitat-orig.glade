<?xml version="1.0" standalone="no"?> <!--*- mode: nxml -*-->
<!DOCTYPE glade-interface SYSTEM "http://glade.gnome.org/glade-2.0.dtd" >

<glade-interface>
  <widget class="GtkWindow" id="baseWindow">
    <property name="title" translatable="yes">Habitat</property>
    <property name="type">GTK_WINDOW_TOPLEVEL</property>
    <property name="modal">no</property>
    <property name="allow_shrink">yes</property>
    <property name="allow_grow">yes</property>
    <property name="width-request">750</property>
    <property name="height-request">540</property>
    <property name="window-position">GTK_WIN_POS_NONE</property>
    <property name="visible">yes</property>

    <signal name="destroy_event" handler="on_baseWindow_destroy_event" />
    <signal name="delete_event" handler="on_baseWindow_delete_event" />

    <child>
      <widget class="GtkVBox" id="bars">
        <property name="homogeneous">no</property>
        <property name="spacing">0</property>
        <property name="visible">yes</property>

        <child>
          <widget class="GtkMenuBar" id="menubar">
            <property name="height-request">28</property>
            <property name="visible">yes</property>

            <child>
              <widget class="GtkMenuItem" id="menufile">
                <property name="tooltip" translatable="yes">Click to open files and exit</property>
                <property name="label" translatable="yes">File</property>
                <property name="visible">yes</property>

                <child>
                  <widget class="GtkMenu" id="menufile_menu">
                    <property name="visible">yes</property>

                    <child>
                      <widget class="GtkMenuItem" id="open">
                        <property name="tooltip" translatable="yes">Open a Ringstore data file</property>
                        <property name="label" translatable="yes">_Open...</property>
                        <property name="visible">yes</property>
                        <property name="use_underline">yes</property>

                        <signal name="activate" handler="on_open_activate" />

                        <accelerator key="O" modifiers="GDK_CONTROL_MASK" signal="activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="close">
                        <property name="tooltip" translatable="yes">Close a Ringstore data file</property>
                        <property name="label" translatable="yes">_Close...</property>
                        <property name="visible">yes</property>
                        <property name="use_underline">yes</property>

                        <signal name="activate" handler="on_close_activate" />

                        <accelerator key="C" modifiers="GDK_CONTROL_MASK" signal="activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="host">
                        <property name="tooltip" translatable="yes">Open host data from the repository or direct from the host</property>
                        <property name="label" translatable="yes">_Host...</property>
                        <property name="visible">yes</property>
                        <property name="use_underline">yes</property>

                        <signal name="activate" handler="on_host_activate" />

                        <accelerator key="H" modifiers="GDK_CONTROL_MASK" signal="activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="route">
                        <property name="tooltip" translatable="yes">Open data from a route specification</property>
                        <property name="label" translatable="yes">_Route...</property>
                        <property name="visible">yes</property>
                        <property name="use_underline">yes</property>

                        <signal name="activate" handler="on_route_activate" />

                        <accelerator key="R" modifiers="GDK_CONTROL_MASK" signal="activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="repository">
                        <property name="tooltip" translatable="yes">Update the repository settings</property>
                        <property name="label" translatable="yes">Repository...</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_repository_activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="separator4">
                        <property name="visible">yes</property>
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="exit">
                        <property name="tooltip" translatable="yes">Exit the application</property>
                        <property name="label" translatable="yes">E_xit</property>
                        <property name="visible">yes</property>
                        <property name="use_underline">yes</property>

                        <signal name="activate" handler="on_exit_activate" />

                        <accelerator key="X" modifiers="GDK_CONTROL_MASK" signal="activate" />
                      </widget>
                    </child>
                  </widget>
                </child>
              </widget>
            </child>

            <child>
              <widget class="GtkMenuItem" id="view">
                <property name="label" translatable="yes">View</property>
                <property name="visible">yes</property>

                <child>
                  <widget class="GtkMenu" id="view_menu">
                    <property name="visible">yes</property>

                    <child>
                      <widget class="GtkMenuItem" id="update">
                        <property name="tooltip" translatable="yes">Update view of selected datastore</property>
                        <property name="label" translatable="yes">Update View</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_update_activate" />

                        <accelerator key="L" modifiers="GDK_CONTROL_MASK" signal="activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="update_node">
                        <property name="tooltip" translatable="yes">Update selected route in choice tree</property>
                        <property name="label" translatable="yes">Update Current Choice</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_update_node_activate" />

                        <accelerator key="F9" modifiers="0" signal="activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="update_all_nodes">
                        <property name="tooltip" translatable="yes">Update all routes in the choice tree</property>
                        <property name="label" translatable="yes">Update All Choices</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_update_all_nodes_activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="separator5">
                        <property name="visible">yes</property>
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkCheckMenuItem" id="raw_data">
                        <property name="tooltip" translatable="yes">Include raw data in choice tree</property>
                        <property name="label" translatable="yes">Browse Raw Data</property>
                        <property name="active">yes</property>
                        <property name="always_show_toggle">yes</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_raw_data_activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="separator8">
                        <property name="visible">yes</property>
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="view_logs">
                        <property name="tooltip" translatable="yes">Popup window of application logs</property>
                        <property name="label" translatable="yes">View Logs...</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_view_logs_activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="save_viewed_data">
                        <property name="tooltip" translatable="yes">Write the data shown in the graph or table to a file</property>
                        <property name="label" translatable="yes">Save Viewed Data...</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_save_viewed_data_activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="send_data_to_application">
                        <property name="tooltip" translatable="yes">Send viewed data to an application</property>
                        <property name="label" translatable="yes">Send Data to Application...</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_send_data_to_application_activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="send_data_to_email">
                        <property name="tooltip" translatable="yes">Send viewed data using an e-mail</property>
                        <property name="label" translatable="yes">Send Data via E-mail...</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_send_data_to_email_activate" />
                      </widget>
                    </child>
                  </widget>
                </child>
              </widget>
            </child>

            <child>
              <widget class="GtkMenuItem" id="collect">
                <property name="label" translatable="yes">Collect</property>
                <property name="visible">yes</property>

                <child>
                  <widget class="GtkMenu" id="collect_menu">
                    <property name="visible">yes</property>

                    <child>
                      <widget class="GtkMenuItem" id="local_collect">
                        <property name="tooltip" translatable="yes">Starts or stops collection of local public data</property>
                        <property name="label" translatable="yes">Local Data...</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_local_collect_activate" />
                      </widget>
                    </child>
                  </widget>
                </child>
              </widget>
            </child>

            <child>
              <widget class="GtkMenuItem" id="menugraph">
                <property name="tooltip" translatable="yes">Graph manipulation functions</property>
                <property name="label" translatable="yes">Graph</property>
                <property name="visible">yes</property>

                <child>
                  <widget class="GtkMenu" id="menugraph_menu">
                    <property name="visible">yes</property>

                    <child>
                      <widget class="GtkMenuItem" id="zoom_in">
                        <property name="label" translatable="yes">Zoom In</property>
                        <property name="visible">yes</property>

                        <child>
                          <widget class="GtkMenu" id="zoom_in_menu">
                            <property name="visible">yes</property>

                            <child>
                              <widget class="GtkMenuItem" id="zoom_in_horizontally">
                                <property name="label" translatable="yes">Horizontally</property>
                                <property name="visible">yes</property>

                                <signal name="activate" handler="on_zoom_in_horizontally_activate" />

                                <accelerator key="plus" modifiers="GDK_SHIFT_MASK" signal="activate" />
                              </widget>
                            </child>

                            <child>
                              <widget class="GtkMenuItem" id="zoom_in_vertically">
                                <property name="label" translatable="yes">Vertically</property>
                                <property name="visible">yes</property>

                                <signal name="activate" handler="on_zoom_in_vertically_activate" />

                                <accelerator key="asciicircum" modifiers="GDK_SHIFT_MASK" signal="activate" />
                              </widget>
                            </child>
                          </widget>
                        </child>
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="zoom_out">
                        <property name="label" translatable="yes">Zoom out</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_zoom_out_activate" />

                        <accelerator key="minus" modifiers="0" signal="activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="show_rulers">
                        <property name="tooltip" translatable="yes">Show ruler measurements on the border of the graph window</property>
                        <property name="label" translatable="yes">Show Rulers</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_show_rulers_activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="hide_rulers">
                        <property name="tooltip" translatable="yes">Hide ruler measurements on the border of the graph window</property>
                        <property name="label" translatable="yes">Hide Rulers</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_hide_rulers_activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="show_axis">
                        <property name="tooltip" translatable="yes">Show graph axis with labels</property>
                        <property name="label" translatable="yes">Show Axis</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_show_axis_activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="hide_axis">
                        <property name="tooltip" translatable="yes">Hide graph axis</property>
                        <property name="label" translatable="yes">Hide Axis</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_hide_axis_activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="view_histogram">
                        <property name="tooltip" translatable="yes">View graph data as a histogram</property>
                        <property name="label" translatable="yes">View Histogram</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_view_histogram_activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="view_curves">
                        <property name="tooltip" translatable="yes">View graph data as curves</property>
                        <property name="label" translatable="yes">View Curves</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_view_curves_activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="set_curve_colour">
                        <property name="tooltip" translatable="yes">Customise the graph colours</property>
                        <property name="label" translatable="yes">Customise Curve Colour</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_set_curve_colour_activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="save_graph_image">
                        <property name="tooltip" translatable="yes">Save the graph to disk as an image</property>
                        <property name="label" translatable="yes">Save Graph Image...</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_save_graph_image_activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="save_graph_setup">
                        <property name="tooltip" translatable="yes">Save graph configuration and viewing details to disk</property>
                        <property name="label" translatable="yes">Save Graph Setup</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_save_graph_setup_activate" />
                      </widget>
                    </child>
                  </widget>
                </child>
              </widget>
            </child>

            <child>
              <widget class="GtkMenuItem" id="menuhelp">
                <property name="tooltip" translatable="yes">Click here to get help</property>
                <property name="label" translatable="yes">Help</property>
                <property name="visible">yes</property>

                <child>
                  <widget class="GtkMenu" id="menuhelp_menu">
                    <property name="visible">yes</property>

                    <child>
                      <widget class="GtkMenuItem" id="logging_level">
                        <property name="label" translatable="yes">Logging level</property>
                        <property name="visible">yes</property>

                        <child>
                          <widget class="GtkMenu" id="logging_level_menu">
                            <property name="visible">yes</property>

                            <child>
                              <widget class="GtkMenuItem" id="logging_level_normal">
                                <property name="label" translatable="yes">Normal (Information)</property>
                                <property name="visible">yes</property>

                                <signal name="activate" handler="on_logging_level_normal_activate" />
                              </widget>
                            </child>

                            <child>
                              <widget class="GtkMenuItem" id="logging_level_high">
                                <property name="label" translatable="yes">High (diagnostic)</property>
                                <property name="visible">yes</property>

                                <signal name="activate" handler="on_logging_level_high_activate" />
                              </widget>
                            </child>

                            <child>
                              <widget class="GtkMenuItem" id="logging_level_higher">
                                <property name="label" translatable="yes">Higher (debug)</property>
                                <property name="visible">yes</property>

                                <signal name="activate" handler="on_logging_level_higher_activate" />
                              </widget>
                            </child>
                          </widget>
                        </child>
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="separator7">
                        <property name="visible">yes</property>
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="readme">
                        <property name="label" translatable="yes">README</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_readme_activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="user_manual">
                        <property name="tooltip" translatable="yes">How to use this application</property>
                        <property name="label" translatable="yes">User Manual</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_user_manual_activate" />
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="manual_pages">
                        <property name="label" translatable="yes">Manual Pages</property>
                        <property name="visible">yes</property>

                        <child>
                          <widget class="GtkMenu" id="manual_pages_menu">
                            <property name="visible">yes</property>

                            <child>
                              <widget class="GtkMenuItem" id="manual_pages_ghabitat">
                                <property name="label" translatable="yes">ghabitat (1)</property>
                                <property name="visible">yes</property>

                                <signal name="activate" handler="on_manual_pages_ghabitat_activate" />
                              </widget>
                            </child>

                            <child>
                              <widget class="GtkMenuItem" id="manual_pages_clockwork">
                                <property name="label" translatable="yes">clockwork (1)</property>
                                <property name="visible">yes</property>

                                <signal name="activate" handler="on_manual_pages_clockwork_activate" />
                              </widget>
                            </child>

                            <child>
                              <widget class="GtkMenuItem" id="manual_pages_habget">
                                <property name="label" translatable="yes">habget (1)</property>
                                <property name="visible">yes</property>

                                <signal name="activate" handler="on_manual_pages_habget_activate" />
                              </widget>
                            </child>

                            <child>
                              <widget class="GtkMenuItem" id="manual_pages_habput">
                                <property name="label" translatable="yes">habput (1)</property>
                                <property name="visible">yes</property>

                                <signal name="activate" handler="on_manual_pages_habput_activate" />
                              </widget>
                            </child>

                            <child>
                              <widget class="GtkMenuItem" id="manual_pages_configuration">
                                <property name="label" translatable="yes">configuration (5)</property>
                                <property name="visible">yes</property>

                                <signal name="activate" handler="on_manual_pages_configuration_activate" />
                              </widget>
                            </child>
                          </widget>
                        </child>
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="separator1">
                        <property name="visible">yes</property>
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="web_systemgardencom">
                        <property name="label" translatable="yes">SystemGarden.com</property>
                        <property name="visible">yes</property>

                        <child>
                          <widget class="GtkMenu" id="web_systemgardencom_menu">
                            <property name="visible">yes</property>

                            <child>
                              <widget class="GtkMenuItem" id="web_system_garden">
                                <property name="tooltip" translatable="yes">Describes the creators</property>
                                <property name="label" translatable="yes">Home</property>
                                <property name="visible">yes</property>

                                <signal name="activate" handler="on_web_system_garden_activate" />
                              </widget>
                            </child>

                            <child>
                              <widget class="GtkMenuItem" id="web_habitat">
                                <property name="label" translatable="yes">Habitat</property>
                                <property name="visible">yes</property>

                                <signal name="activate" handler="on_web_habitat_activate" />
                              </widget>
                            </child>

                            <child>
                              <widget class="GtkMenuItem" id="web_harvest">
                                <property name="tooltip" translatable="yes">Describes the respository and analysis system</property>
                                <property name="label" translatable="yes">Harvest</property>
                                <property name="visible">yes</property>

                                <signal name="activate" handler="on_web_harvest_activate" />
                              </widget>
                            </child>
                          </widget>
                        </child>
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="separator2">
                        <property name="visible">yes</property>
                      </widget>
                    </child>

                    <child>
                      <widget class="GtkMenuItem" id="about_habitat">
                        <property name="tooltip" translatable="yes">Describes this application</property>
                        <property name="label" translatable="yes">About Habitat...</property>
                        <property name="visible">yes</property>

                        <signal name="activate" handler="on_about_habitat_activate" />
                      </widget>
                    </child>
                  </widget>
                </child>
              </widget>
            </child>
          </widget>
          <packing>
            <property name="padding">0</property>
            <property name="expand">no</property>
            <property name="fill">yes</property>
          </packing>
        </child>

        <child>
          <widget class="GtkHPaned" id="panes">
            <property name="tooltip" translatable="yes">Drag handle between tree and data sections to change their display areas</property>
            <property name="width-request">750</property>
            <property name="height-request">500</property>
            <property name="visible">yes</property>

            <child>
              <widget class="GtkScrolledWindow" id="treescrollwin">
                <property name="hscrollbar_policy">GTK_POLICY_AUTOMATIC</property>
                <property name="vscrollbar_policy">GTK_POLICY_AUTOMATIC</property>
                <property name="width-request">200</property>
                <property name="visible">yes</property>

                <child>
                  <widget class="GtkViewport" id="treeviewport">
                    <property name="shadow_type">GTK_SHADOW_NONE</property>
                    <property name="visible">yes</property>

                    <child>
                      <widget class="GtkTreeView" id="tree">
                        <property name="tooltip" translatable="yes">Select choices in the tree to change the data display on the right</property>
                        <property name="can_focus">yes</property>
                        <property name="shadow_type">GTK_SHADOW_IN</property>
                        <property name="headers-visible">no</property>
                        <property name="visible">yes</property>

                        <signal name="tree_select_row" handler="on_tree_select_row" />
                        <signal name="tree_expand" handler="on_tree_expand" />
                        <signal name="button_press_event" handler="on_tree_button_press_event" />

                        <child>
                          <widget class="GtkLabel" id="label7">
                            <property name="child_name">CTree:title</property>
                            <property name="label" translatable="yes">label7</property>
                            <property name="justify">GTK_JUSTIFY_CENTER</property>
                            <property name="wrap">no</property>
                            <property name="xalign">0.5</property>
                            <property name="yalign">0.5</property>
                            <property name="xpad">0</property>
                            <property name="ypad">0</property>
                            <property name="visible">yes</property>
                          </widget>
                        </child>
                      </widget>
                    </child>
                  </widget>
                </child>

                <child internal-child="hscrollbar">
                  <widget class="GtkHScrollbar" id="convertwidget1">
                    <property name="update_policy">GTK_UPDATE_CONTINUOUS</property>
                    <property name="visible">yes</property>
                  </widget>
                </child>

                <child internal-child="vscrollbar">
                  <widget class="GtkVScrollbar" id="convertwidget2">
                    <property name="update_policy">GTK_UPDATE_CONTINUOUS</property>
                    <property name="visible">yes</property>
                  </widget>
                </child>
              </widget>
              <packing>
                <property name="shrink">yes</property>
                <property name="resize">no</property>
              </packing>
            </child>

            <child>
              <widget class="GtkVBox" id="presentationbox">
                <property name="homogeneous">no</property>
                <property name="spacing">0</property>
                <property name="visible">yes</property>

                <child>
                  <widget class="GtkFrame" id="graphframe">
                    <property name="visible">no</property>
                    <property name="label_xalign">0</property>
                    <property name="shadow">GTK_SHADOW_ETCHED_IN</property>

                    <child>
                      <widget class="GtkHPaned" id="graphpanes">
                        <property name="position">400</property>
                        <property name="visible">yes</property>

                        <child>
                          <widget class="GtkTreeView" id="graph">
                            <property name="visible">yes</property>
                          </widget>
                          <packing>
                            <property name="shrink">yes</property>
                            <property name="resize">no</property>
                          </packing>
                        </child>

                        <child>
                          <widget class="GtkHandleBox" id="ctlhandbox">
                            <property name="handle_position">GTK_POS_TOP</property>
                            <property name="snap_edge">GTK_POS_TOP</property>
                            <property name="shadow">GTK_SHADOW_OUT</property>
                            <property name="visible">yes</property>

                            <child>
                              <widget class="GtkVBox" id="ctlbox">
                                <property name="homogeneous">no</property>
                                <property name="spacing">0</property>
                                <property name="visible">yes</property>

                                <child>
                                  <widget class="GtkFrame" id="ctlframe">
                                    <property name="label" translatable="yes">Zoom &amp; Scale</property>
                                    <property name="label_xalign">0</property>
                                    <property name="shadow">GTK_SHADOW_ETCHED_IN</property>
                                    <property name="visible">yes</property>

                                    <child>
                                      <widget class="GtkTable" id="ctltab">
